R_100;
RR_100;
/* [wxMaxima: input   end   ] */


/* [wxMaxima: input   start ] */
wxsubscripts: false$
RR_xy;
R_100;
RR_100;
/* [wxMaxima: input   end   ] */


/* [wxMaxima: input   start ] */
